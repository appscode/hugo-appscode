{
  "hosting": [
    {
      "target": "hugo",
      "public": "public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "headers": [
        {
          "headers": [
            {
              "key": "X-XSS-Protection",
              "value": "1; mode=block"
            },
            {
              "key": "X-Content-Type-Options",
              "value": "nosniff"
            }
          ],
          "source": "**"
        },
        {
          "headers": [
            {
              "key": "Cache-Control",
              "value": "public, max-age=86400"
            }
          ],
          "source": "**"
        },
        {
          "headers": [
            {
              "key": "Cache-Control",
              "value": "public, max-age=86400"
            }
          ],
          "source": "*"
        },
        {
          "headers": [
            {
              "key": "Cache-Control",
              "value": "max-age=300"
            }
          ],
          "source": "404.html"
        }
      ],
      "redirects": [
        {
          "source": "/career/",
          "destination": "https://appscode.zohorecruit.com",
          "type": 302
        },
        {
          "source": "/contact/sales/",
          "destination": "/contact/",
          "type": 302
        },
        {
          "source": "/products/guard/latest/",
          "destination": "/products/guard/v0.6.1/",
          "type": 302
        },
        {
          "source": "/products/guard/latest/:rest*",
          "destination": "/products/guard/v0.6.1/:rest",
          "type": 302
        },
        {
          "source": "/products/kubed/latest/",
          "destination": "/products/kubed/v0.12.0/",
          "type": 302
        },
        {
          "source": "/products/kubed/latest/:rest*",
          "destination": "/products/kubed/v0.12.0/:rest",
          "type": 302
        },
        {
          "source": "/products/searchlight/latest/",
          "destination": "/products/searchlight/8.0.0/",
          "type": 302
        },
        {
          "source": "/products/searchlight/latest/:rest*",
          "destination": "/products/searchlight/8.0.0/:rest",
          "type": 302
        },
        {
          "source": "/products/service-broker/latest/",
          "destination": "/products/service-broker/0.3.1/",
          "type": 302
        },
        {
          "source": "/products/service-broker/latest/:rest*",
          "destination": "/products/service-broker/0.3.1/:rest",
          "type": 302
        },
        {
          "source": "/products/swift/latest/",
          "destination": "/products/swift/v0.12.1/",
          "type": 302
        },
        {
          "source": "/products/swift/latest/:rest*",
          "destination": "/products/swift/v0.12.1/:rest",
          "type": 302
        },
        {
          "source": "/products/cloud-deployment",
          "destination": "https://pharmer.dev",
          "type": 301
        },
        {
          "source": "/products/pharmer",
          "destination": "https://pharmer.dev",
          "type": 301
        },
        {
          "source": "/products/stash",
          "destination": "https://stash.run",
          "type": 301
        },
        {
          "source": "/products/stash/:rest*",
          "destination": "https://stash.run/docs/:rest",
          "type": 302
        },
        {
          "source": "/products/voyager",
          "destination": "https://voyagermesh.com",
          "type": 301
        },
        {
          "source": "/products/voyager/:rest*",
          "destination": "https://voyagermesh.com/docs/:rest",
          "type": 302
        }
      ]
    }
  ]
}
